          }
        },
        "story": {
          "name": "railway",
          "username": "postgres",
          "password": {
            "$env": "PGPASSWORD"
          }
        },
        "doku": {
          "name": "railway",
          "username": "postgres",
          "password": {
            "$env": "PGPASSWORD"
          }
        },
        "ai": {
          "name": "railway",
          "username": "postgres",
          "password": {
            "$env": "PGPASSWORD"
          }
        }
      }
    }
  ],
  "secrets": {
