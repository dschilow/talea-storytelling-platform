{
  "$schema": "https://encore.dev/schemas/infra.schema.json",
  "sql_servers": [
    {
      "host": "postgres.railway.internal:5432",
      "tls_config": {
        "disable_ca_validation": true
      },
      "databases": {
        "user": {
          "name": "railway",
          "username": "postgres",
          "password": {
            "$env": "PGPASSWORD"
          }
        },
        "avatar": {
          "name": "railway",
          "username": "postgres",
          "password": {
            "$env": "PGPASSWORD"
          }
        },
        "story": {
          "name": "railway",
          "username": "postgres",
          "password": {
            "$env": "PGPASSWORD"
          }
        },
        "doku": {
          "name": "railway",
          "username": "postgres",
          "password": {
            "$env": "PGPASSWORD"
          }
        },
        "personality_tracking": {
          "name": "railway",
          "username": "postgres",
          "password": {
            "$env": "PGPASSWORD"
          }
        },
        "log": {
          "name": "railway",
          "username": "postgres",
          "password": {
            "$env": "PGPASSWORD"
          }
        },
        "fairytales": {
          "name": "railway",
          "username": "postgres",
          "password": {
            "$env": "PGPASSWORD"
          }
        }
      }
    }
  ],
  "buckets": [],
  "secrets": {
    "ClerkSecretKey": {
      "$env": "CLERK_SECRET_KEY"
    },
    "OpenAIKey": {
      "$env": "OPENAI_API_KEY"
    },
    "RunwareApiKey": {
      "$env": "RUNWARE_API_KEY"
    },
    "MCPServerAPIKey": {
      "$env": "MCP_SERVER_API_KEY"
    },
    "GeminiAPIKey": {
      "$env": "GEMINI_API_KEY"
    },
    "BucketEndpoint": {
      "$env": "BUCKET_ENDPOINT"
    },
    "BucketRegion": {
      "$env": "BUCKET_REGION"
    },
    "BucketName": {
      "$env": "BUCKET_NAME"
    },
    "BucketAccessKeyId": {
      "$env": "BUCKET_ACCESS_KEY_ID"
    },
    "BucketSecretAccessKey": {
      "$env": "BUCKET_SECRET_ACCESS_KEY"
    },
    "BucketPublicBaseUrl": {
      "$env": "BUCKET_PUBLIC_BASE_URL"
    },
    "BucketForcePathStyle": {
      "$env": "BUCKET_FORCE_PATH_STYLE"
    },
    "BucketUploadMode": {
      "$env": "BUCKET_UPLOAD_MODE"
    },
    "BucketAccessMode": {
      "$env": "BUCKET_ACCESS_MODE"
    },
    "BucketSignedUrlTtlSeconds": {
      "$env": "BUCKET_SIGNED_URL_TTL"
    }
  }
}
