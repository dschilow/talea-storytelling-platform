{
  "$schema": "https://encore.dev/schemas/infra.schema.json",
  "sql_servers": [
    {
      "host": "$PGHOST",
      "databases": {
        "avatar": {
          "database_name": "railway",
          "username": "$PGUSER",
          "password": "$PGPASSWORD"
        },
        "doku": {
          "database_name": "railway",
          "username": "$PGUSER",
          "password": "$PGPASSWORD"
        },
        "personality_tracking": {
          "database_name": "railway",
          "username": "$PGUSER",
          "password": "$PGPASSWORD"
        },
        "story": {
          "database_name": "railway",
          "username": "$PGUSER",
          "password": "$PGPASSWORD"
        },
        "user": {
          "database_name": "railway",
          "username": "$PGUSER",
          "password": "$PGPASSWORD"
        }
      }
    }
  ],
  "object_storage": [
    {
      "s3": {
        "region": "us-east-1",
        "endpoint": "$S3_ENDPOINT",
        "access_key_id": "$S3_ACCESS_KEY_ID",
        "secret_access_key": "$S3_SECRET_ACCESS_KEY"
      },
      "buckets": {
        "avatales-ai-logs": {
          "bucket_name": "avatales-ai-logs"
        }
      }
    }
  ],
  "secrets": {
    "ClerkSecretKey": "$CLERK_SECRET_KEY",
    "OpenAIKey": "$OPENAI_API_KEY",
    "RunwareApiKey": "$RUNWARE_API_KEY"
  }
}
